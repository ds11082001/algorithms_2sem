#include <iostream>
#include <vector>
#include <queue>

std::vector<int> bfs(std::vector<std::vector<int>> g, int u) {
    int n = g.size();
    std::vector<int> dist(n, 6e8);
    std::queue<int> q;
    q.push(u);
    dist[u] = 0;
    while (not q.empty()) {
        int v = q.front();
        q.pop();
        for (int to : g[v]) {
            if (dist[to] > dist[v] + 1) {
                q.push(to);
                dist[to] = dist[v] + 1;
            }
        }
    }
    return dist;
}

int main() {
    int n, m;
    std::cin >> n >> m;
    int a, b, c;
    std::cin >> a >> b >> c;
    a--;
    b--;
    c--;
    std::vector<std::vector<int>> g(n);
    for (int i = 0; i < m; ++i) {
        int from, to;
        std::cin >> from >> to;
        from--;
        to--;
        g[from].push_back(to);
        g[to].push_back(from);
    }
    std::vector<int> distA = bfs(g, a);
    std::vector<int> distB = bfs(g, b);
    std::vector<int> distC = bfs(g, c);
    int minDist = 6e8;
    for (int i = 0; i < n; ++i) {
        minDist = std::min(minDist, distA[i] + distB[i] + distC[i]);
    }
    std::cout << minDist;
    return 0;
}

